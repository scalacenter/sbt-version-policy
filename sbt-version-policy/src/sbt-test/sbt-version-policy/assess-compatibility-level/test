# Publish the first version
> v1_0_0/publishLocal
> reload

# Assess the compatibility level of the next iteration
> v1_0_1/checkAssessedCompatibilityIsBinaryAndSourceCompatible
# Now that we know we are binary and source compatible, we can set its version and publish it
> set v1_0_1/version := "1.0.1"
> v1_0_1/publishLocal
> reload

> v1_0_2/checkAssessedCompatibilityIsBinaryAndSourceCompatible
> set v1_0_2/version := "1.0.2"
> v1_0_2/publishLocal
> reload

> v1_1_0/checkAssessedCompatibilityIsBinaryCompatible
> set v1_1_0/version := "1.1.0"
> v1_1_0/publishLocal
> reload

> v1_2_0/checkAssessedCompatibilityIsBinaryCompatible
> set v1_2_0/version := "1.2.0"
> v1_2_0/publishLocal
> reload

> v2_0_0/checkAssessedCompatibilityIsNone
> set v2_0_0/version := "2.0.0"
> v2_0_0/publishLocal
> reload

> v3_0_0/checkAssessedCompatibilityIsNone
